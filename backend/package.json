    "pg": "^7.4.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "https://github.com/chaijs/chai-http#832f6c2d9b2252f57db7c5173c421aa5c9c45579",
